<%= link_to 'Login', login_users_path %> | <%= link_to 'Signup', signup_users_path %>

<h2>Login</h2>
<%= @msg %>

<%= form_tag("", method: "get") do %>
  <p>
    <%= label_tag(:email, "Your Email")%><br />
    <%= text_field_tag(:email) %>
   
  </p>
  <p>
    <%= label_tag(:password, "Your password")%><br />
    <%= password_field_tag(:password) %>
  </p>
  <p><%= submit_tag ("Login") %></p>

<% end %>